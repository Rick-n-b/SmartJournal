<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="300.0" maxWidth="600.0" minHeight="251.0" minWidth="409.0" prefHeight="251.0" prefWidth="409.0" xmlns="http://javafx.com/javafx/21.0.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nstu.logbook.controllers.AskPageController">
   <children>
      <Button fx:id="cancelButton" layoutX="42.0" layoutY="137.0" mnemonicParsing="false" onAction="#cancel" prefHeight="40.0" prefWidth="100.0" text="Cancel">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="saveButton" layoutX="154.0" layoutY="137.0" mnemonicParsing="false" onAction="#save" prefHeight="40.0" prefWidth="100.0" text="Save">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label fx:id="messageLabel" alignment="CENTER" layoutX="42.0" layoutY="21.0" prefHeight="109.0" prefWidth="326.0" text="Save everything to cloud storage or download everything from storage (both options COMPLETELY overwrite the data)" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="downloadButton" layoutX="268.0" layoutY="137.0" mnemonicParsing="false" onAction="#download" prefHeight="40.0" prefWidth="100.0" text="Download">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
